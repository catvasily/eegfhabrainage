		"fmax": 100.0,
		"ymin": 1e-1,
		"ymax": 2e1,
                "fstep": 10.0,
		"spect_log_x": true,
		"spect_log_y": true,
		"n_fft": 1024,
		"kwargs": {
			"average": false,
			"dB": false,
			"amplitude": true,
			"xscale": "linear",
			"ci": "sd",
			"ci_alpha": 0.3,
			"color": "black",
			"alpha": null,
			"spatial_colors": false,
			"sphere": null,
			"exclude": null,
			"axes": null,
			"show": false
			}
